#!/usr/bin/bash

#wget "https://proxylist.geonode.com/api/proxy-list?limit=500&page=2&sort_by=lastChecked&sort_type=desc"

rm -vf proxy-list.*

set -ex

for page in {01..14}; do
	echo "Downloading page $page"
	wget "https://proxylist.geonode.com/api/proxy-list?limit=500&page=${page}&sort_by=lastChecked&sort_type=desc" -O "proxy-list.${page}.json"
done